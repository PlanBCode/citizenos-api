        "freezeTableName": false,
        "syncOnAssociation": false,
        "charset": "utf8",
        "collate": "utf8_general_ci",
        "timestamps": true,
        "paranoid": true
      },
      "omitNull": false,
      "dialectOptions": {
        "ssl": true
